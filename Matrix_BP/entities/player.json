                                            "value": "team:blue"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:blue"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:cyan"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:cyan"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:yellow"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:yellow"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:orange"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:orange"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:green"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:green"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:purple"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:purple"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:white"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:white"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:black"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:black"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:gray"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:gray"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:lime"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:lime"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:brown"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:brown"
                                        }
                                    ]
                                }
                            ]
                        }
                    },
                    "deals_damage": "no",
                    "cause": "all"
                }
            }
        },

        "events": {
            "minecraft:gain_raid_omen": {
                "add": {
                    "component_groups": ["minecraft:raid_omen"]
                }
            },
            "matrix:tempkick": {
                "add": {
                    "component_groups": ["matrix:tempkick"]
                }
            },
            "matrix:unvanish": {
                "add": {
                    "component_groups": ["matrix:unvanish"]
                },
                "queue_command": {
                    "command": ["tag @s remove matrix:impeccable", "tag @s remove matrix:vanished", "effect @s minecraft:invisibility infinite 0 true"],
                    "target": "self"
                }
            },
            "matrix:vanish": {
                "add": {
                    "component_groups": ["matrix:vanish"]
                },
                "queue_command": {
                    "command": ["tag @s add matrix:impeccable", "tag @s add matrix:vanished"],
                    "target": "self"
                }
            },
            "matrix:container_opened": {
                "queue_command": {
                    "command": "tag @s add matrix:container_opened",
                    "target": "self"
                }
            },
            "matrix:container_closed": {
                "queue_command": {
                    "command": "tag @s remove matrix:container_opened",
                    "target": "self"
                }
            },
            "matrix:despawn": {
                "add": {
                    "component_groups": ["matrix:despawn"]
                }
            },
            "matrix:dummy_gethit": {
                "add": {
                    "component_groups": ["matrix:dummy_gethit"]
                }
            },
            "matrix:transfer_to_dummy": {
                "add": {
                    "component_groups": ["matrix:transfer_to_dummy"]
                }
            }
        }
    }
