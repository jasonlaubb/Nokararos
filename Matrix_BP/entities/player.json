{
    "format_version": "1.18.20",
    "minecraft:entity": {
        "description": {
            "identifier": "minecraft:player",
            "is_spawnable": false,
            "is_summonable": true, // For adding a new dummy
            "is_experimental": false,
            "scripts": {
                "animate": [
                    "may_fly",
                    "alive",
                    "gliding",
                    "jumping",
                    "levitating",
                    "moving",
                    "attack_time",
                    "on_ground",
                    "riding",
                    "sneaking",
                    "sprinting",
                    "swimming",
                    "using_item",
                    "eating",
                    "emoting",
                    "digging",
                    "ignited",
                    "wet",
                    "north",
                    "east",
                    "south",
                    "west"
                ]
            },
            "animations": {
                "may_fly": "controller.animation.may_fly",
                "alive": "controller.animation.alive",
                "attack_time": "controller.animation.attack_time",
                "gliding": "controller.animation.gliding",
                "jumping": "controller.animation.jumping",
                "levitating": "controller.animation.levitating",
                "moving": "controller.animation.moving",
                "on_ground": "controller.animation.on_ground",
                "riding": "controller.animation.riding",
                "sneaking": "controller.animation.sneaking",
                "sprinting": "controller.animation.sprinting",
                "swimming": "controller.animation.swimming",
                "sleeping": "controller.animation.sleeping",
                "using_item": "controller.animation.using_item",
                "eating": "controller.animation.eating",
                "emoting": "controller.animation.emoting",
                "digging": "controller.animation.digging",
                "ignited": "controller.animation.ignited",
                "wet": "controller.animation.wet",
                "north": "controller.animation.facing_north",
                "east": "controller.animation.facing_east",
                "south": "controller.animation.facing_south",
                "west": "controller.animation.facing_west"
            }
        },

        "component_groups": {
            "minecraft:add_bad_omen": {
                "minecraft:spell_effects": {
                    "add_effects": [
                        {
                            "effect": "bad_omen",
                            "duration": 6000,
                            "visible": false,
                            "display_on_screen_animation": true
                        }
                    ]
                },
                "minecraft:timer": {
                    "time": [0.0, 0.0],
                    "looping": false,
                    "time_down_event": {
                        "event": "minecraft:clear_add_bad_omen",
                        "target": "self"
                    }
                }
            },
            "minecraft:gain_raid_omen": {
                "minecraft:spell_effects": {
                    "add_effects": [
                        {
                            "effect": "raid_omen",
                            "duration": 30,
                            "visible": false,
                            "display_on_screen_animation": true
                        }
                    ],
                    "remove_effects": ["bad_omen"]
                }
            },
            "matrix:unvanish": {
                "minecraft:scale": {
                    "value": 1
                },
                "minecraft:attack": {
                    "damage": 1
                },
                "minecraft:breathable": {
                    "breathes_solids": false,
                    "total_supply": 15,
                    "suffocate_time": -1,
                    "inhale_time": 3.75,
                    "generates_bubbles": false
                }
            },
            "matrix:vanish": {
                "minecraft:scale": {
                    "value": 0.0
                },
                "minecraft:attack": {
                    "damage": 0.0
                },
                "minecraft:breathable": {
                    "breathes_solids": true,
                    "total_supply": 15,
                    "suffocate_time": -1,
                    "inhale_time": 3.75,
                    "generates_bubbles": false
                }
            },
            "matrix:tempkick": {
                "minecraft:explode": {
                    "fuse_length": 0,
                    "fuse_lit": true,
                    "power": 0,
                    "causes_fire": false,
                    "breaks_blocks": false
                }
            },
            "matrix:transfer_to_dummy": {
                "minecraft:damage_sensor": {
                    "triggers": {
                        "on_damage": {
                            "event": "matrix:dummy_gethit"
                        }
                    }
                },
                // Kill the entities after 3 seconds
                "minecraft:timer": {
                    "time": 3,
                    "looping": false,
                    "time_down_event": {
                        "event": "matrix:despawn",
                        "target": "self"
                    }
                },
                "minecraft:spell_effects": {
                    // Add the slow falling effect (prevent the entity drop the void)
                    "add_effects": [
                        {
                            "duration": 5,
                            "effect": "levitation",
                            "visible": false,
                            "amplifier": 0
                        }
                    ]
                }
            },
            "matrix:despawn": {
                "minecraft:instant_despawn": {
                    "remove_child_entities": true
                }
            },
            "matrix:dummy_gethit": {
                // Despawn the entity after a tick after the entity already get hit
                "minecraft:timer": {
                    "time": 0.05,
                    "looping": false,
                    "time_down_event": {
                        "event": "matrix:despawn",
                        "target": "self"
                    }
                }
            }
        },

        "components": {
            "minecraft:experience_reward": {
                "on_death": "Math.Min(query.player_level * 7, 100)"
            },
            "minecraft:type_family": {
                "family": ["player"]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:loot": {
                "table": "loot_tables/empty.json"
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.8
            },
            "minecraft:can_climb": {},
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:attack": {
                "damage": 1
            },
            "minecraft:exhaustion_values": {
                "heal": 6,
                "jump": 0.05,
                "sprint_jump": 0.2,
                "mine": 0.005,
                "attack": 0.1,
                "damage": 0.1,
                "walk": 0.0,
                "sprint": 0.01,
                "swim": 0.01
            },
            "minecraft:player.saturation": {
                "value": 5,
                "max": 20
            },
            "minecraft:player.exhaustion": {
                "value": 0,
                "max": 20
            },
            "minecraft:player.level": {
                "value": 0,
                "max": 24791
            },
            "minecraft:player.experience": {
                "value": 0,
                "max": 1
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": -1,
                "inhale_time": 3.75,
                "generates_bubbles": false
            },
            "minecraft:nameable": {
                "always_show": true,
                "allow_name_tag_renaming": false
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:insomnia": {
                "days_until_insomnia": 3
            },
            "minecraft:rideable": {
                "seat_count": 2,
                "family_types": ["parrot_tame"],
                "pull_in_entities": true,
                "seats": [
                    {
                        "position": [0.4, -0.2, -0.1],
                        "min_rider_count": 0,
                        "max_rider_count": 0,
                        "lock_rider_rotation": 0
                    },
                    {
                        "position": [-0.4, -0.2, -0.1],
                        "min_rider_count": 1,
                        "max_rider_count": 2,
                        "lock_rider_rotation": 0
                    }
                ]
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:block_climber": {},
            "minecraft:environment_sensor": {
                "triggers": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_mob_effect",
                                    "subject": "self",
                                    "value": "bad_omen"
                                },
                                {
                                    "test": "is_in_village",
                                    "subject": "self",
                                    "value": true
                                }
                            ]
                        },
                        "event": "minecraft:gain_raid_omen"
                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_container_open",
                                    "operator": "==",
                                    "subject": "self",
                                    "value": true
                                }
                            ]
                        },
                        "event": "container_opened"
                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_container_open",
                                    "operator": "==",
                                    "subject": "self",
                                    "value": false
                                }
                            ]
                        },
                        "event": "container_closed"
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": {
                    "on_damage": {
                        "filters": {
                            "any_of": [
                                {
                                    "any_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "matrix:pvp-disabled"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "matrix:impeccable"
                                        }
                                    ]
                                },
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "subject": "self",
                                            "value": "team:red"
                                        },
                                        {
                                            "test": "has_tag",
                                            "subject": "other",
                                            "value": "team:red"
                                        }
                                    ]
                                },
                                {
